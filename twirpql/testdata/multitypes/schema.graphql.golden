schema {
	query: Query
}

type Query {
	Hello(req: HelloReq): HelloResp!
	Bye(req: ByeReq): ByeResp!
}

type ByeResp {
	one: Int!
	two: Int!
	three: [Int]!
	four: [Int]!
	traffic: Traffic!
}

type HelloResp {
	one: String!
	two: Boolean!
	three: [String]!
	four: [Boolean]!
}

input ByeReq {
	one: Int!
	two: Int!
	three: [Int]!
	four: [Int]!
}

input HelloReq {
	one: String!
	two: Boolean!
	three: [String]!
	four: [Boolean]!
}

enum Traffic {
	GREEN
	YELLOW
	RED
}
